    "url": "git+https://github.com/forkpoint/davos-cli.git"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "davos": "^2.0.0-alpha.2",
    "progress": "^1.1.8",
    "prompt": "^1.0.0",
    "request-debug": "^0.2.0",
    "underscore": "^1.8.3",
