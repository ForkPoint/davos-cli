{
  "name": "davos-cli",
  "version": "2.0.0-alpha.2",
  "description": "Command-line client for Davos - Salesforce Commerce Cloud (Demandware) cartridge uploader for WebDAV",
  "main": "index.js",
  "scripts": {
    "test": "mocha --opts scripts/mocha.opts src/**/*-test.js",
    "lint": "eslint .",
    "lint:fix": "npm run lint --silent -- --fix",
    "readme": "node ./node_modules/.bin/node-readme",
    "publish-please": "publish-please"
  },
  "bin": {
    "davos": "index.js"
  },
  "engines": {
    "node": ">=8.5.0"
  },
  "keywords": [
    "demandware",
    "webdav",
    "upload",
    "cartridge",
    "salesforce",
    "synch",
    "eclipse",
    "watch",
    "isml",
    "davos",
    "cli",
    "command line",
    "command-line"
  ],
  "author": "ForkPoint",
  "license": "MIT",
  "homepage": "https://github.com/forkpoint/davos-cli#readme",
  "bugs": {
    "url": "https://github.com/forkpoint/davos-cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forkpoint/davos-cli.git"
  },
  "dependencies": {
    "progress": "^2.0.0",
    "prompt": "^1.0.0",
    "request-debug": "^0.2.0",
    "underscore": "^1.9.1",
    "yargonaut": "^1.1.2",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "chai": "~4.1.2",
    "chai-as-promised": "~7.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "fixpack": "~2.3.1",
    "mocha": "~5.2.0",
    "mocha-clean": "~1.0.0",
    "node-readme": "^0.1.9",
    "publish-please": "^3.1.0"
  }
}
